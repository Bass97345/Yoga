<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/acrivites"
    app:startDestination="@id/splashScreen">

    <fragment
        android:id="@+id/splashScreen"
        android:name="com.example.yoga.ui.fragments.SplashScreen"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreen_to_onboarding2"
            app:destination="@id/onboarding" />
        <action
            android:id="@+id/action_splashScreen_to_main2"
            app:destination="@id/main2" />
    </fragment>
    <fragment
        android:id="@+id/onboarding"
        android:name="com.example.yoga.ui.fragments.Onboarding"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/action_onboarding_to_login2"
            app:destination="@id/login" />
        <action
            android:id="@+id/action_onboarding_to_regestration"
            app:destination="@id/regestration" />
    </fragment>
    <fragment
        android:id="@+id/login"
        android:name="com.example.yoga.ui.fragments.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_main2"
            app:destination="@id/main2" />
    </fragment>
    <fragment
        android:id="@+id/main2"
        android:name="com.example.yoga.ui.fragments.Main"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_main2_to_profile"
            app:destination="@id/profile" />
        <action
            android:id="@+id/action_main2_to_menu"
            app:destination="@id/menu" />
        <action
            android:id="@+id/action_main2_to_listener"
            app:destination="@id/listener" />
    </fragment>
    <fragment
        android:id="@+id/regestration"
        android:name="com.example.yoga.ui.fragments.regestration"
        android:label="fragment_regestration"
        tools:layout="@layout/fragment_regestration" >
        <action
            android:id="@+id/action_regestration_to_onboarding"
            app:destination="@id/onboarding" />
    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.example.yoga.ui.fragments.Profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profile_to_main2"
            app:destination="@id/main2" />
        <action
            android:id="@+id/action_profile_to_onboarding"
            app:destination="@id/onboarding" />
        <action
            android:id="@+id/action_profile_to_menu"
            app:destination="@id/menu" />
        <action
            android:id="@+id/action_profile_to_listener"
            app:destination="@id/listener" />
        <action
            android:id="@+id/action_profile_to_bigPhoto2"
            app:destination="@id/bigPhoto2" />
    </fragment>
    <fragment
        android:id="@+id/menu"
        android:name="com.example.yoga.ui.fragments.Menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" />
    <fragment
        android:id="@+id/listener"
        android:name="com.example.yoga.ui.fragments.Listener"
        android:label="fragment_listener"
        tools:layout="@layout/fragment_listener" />
    <fragment
        android:id="@+id/bigPhoto2"
        android:name="com.example.yoga.ui.fragments.BigPhoto"
        android:label="fragment_big_photo"
        tools:layout="@layout/fragment_big_photo" >
        <action
            android:id="@+id/action_bigPhoto2_to_profile"
            app:destination="@id/profile" />
    </fragment>
</navigation>